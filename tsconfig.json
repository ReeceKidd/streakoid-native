{
    "compilerOptions": {
        "target": "es5",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "isolatedModules": true,
        "jsx": "react-native",
        "lib": ["dom", "esnext", "dom.iterable"],
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "resolveJsonModule": true
    },
    "include": ["src", "App.tsx"]
}
